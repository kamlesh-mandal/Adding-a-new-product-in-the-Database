<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Adding a New Product in the Database</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>productInsert</display-name>
    <servlet-name>productInsert</servlet-name>
    <servlet-class>com.servlet.productInsert</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>productInsert</servlet-name>
    <url-pattern>/productInsert</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>productValidation</display-name>
    <servlet-name>productValidation</servlet-name>
    <servlet-class>com.servlet.productValidation</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>productValidation</servlet-name>
    <url-pattern>/productValidation</url-pattern>
  </servlet-mapping>
</web-app>